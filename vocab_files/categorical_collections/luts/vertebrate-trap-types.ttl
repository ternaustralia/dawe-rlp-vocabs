    urnp:categoricalConcept <https://linked.data.gov.au/def/nrm/d1bdaf17-c484-59c6-84a0-8344a5ce5b5c> ;
    urnp:conceptDefinition "The equipment/method used during a passive, 'targeted fauna survey'. Elliott trapping is a technique used to trap small to medium sized mammals. The are usually hinged design that allows trapping to be conducted by folding into a compact panel and easy transport to field locations and storage." ;
    urnp:conceptSource "https://core.vocabs.paratoo.tern.org.au/api/lut-vertebrate-trap-types"^^xsd:anyURI ;
.

<https://linked.data.gov.au/def/nrm/c2013297-ae0c-52fa-a0e3-6473f30edcd4>
    a urnc:CategoricalConceptMeta ;
    schema:url "https://github.com/ternaustralia/dawe-rlp-vocabs/tree/main/vocab_files/categorical_collections/luts/vertebrate-trap-types.ttl"^^xsd:anyURI ;
    urnp:categoricalCollection <https://linked.data.gov.au/def/nrm/fd2ab6f0-01b8-4216-bb7b-1e62eae4a40e> ;
