<https://linked.data.gov.au/def/nrm/fcb4c189-a957-5bba-b3a4-5075b673f87c>
    a urnc:CategoricalConceptMeta ;
    schema:url "https://github.com/ternaustralia/dawe-rlp-vocabs/tree/main/vocab_files/categorical_collections/luts/fauna-sign-types.ttl"^^xsd:anyURI ;
